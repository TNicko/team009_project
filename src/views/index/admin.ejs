<%- include('../partials/navbar.ejs') %>

<div class="main_section" id="admin_section">
    <div class="admin_top">
        <div class="admin_panel">
            <div class="strike_line sl1">

            </div>
            <div class="panel_info">
                <div class="panel_icon pi1">
                    <i class="fa-solid fa-clock"></i>
                </div>
                <div class="panel_title">
                    <p>Unassigned</p>
                    <p><%= open_total %></p>
                </div>

            </div>


        </div>
        <div class="admin_panel">
            <div class="strike_line sl2">

            </div>
            <div class="panel_info">
                <div class="panel_icon pi2">
                    <i class="fa-solid fa-pen"></i>
                </div>
                <div class="panel_title">
                    <p>Assigned</p>
                    <p><%= assigned_total %> </p>
                </div>

            </div>

        </div>
        <div class="admin_panel">
            <div class="strike_line sl3">

            </div>
            <div class="panel_info">
                <div class="panel_icon pi3">
                    <i class="fa-solid fa-triangle-exclamation"></i>
                </div>
                <div class="panel_title">
                    <p>Overdue</p>
                    <p>0</p>
                </div>

            </div>

        </div>
        <div class="admin_panel">
            <div class="strike_line sl4">

            </div>
            <div class="panel_info">
                <div class="panel_icon pi4">
                    <i class="fa-solid fa-gear"></i>
                </div>
                <div class="panel_title">
                    <p>Total</p>
                    <p><%= ticket_total %> </p>
                </div>

            </div>

        </div>

    </div>
    <div class="main_container">

        <%- include('../partials/ticket_table.ejs') %>

    </div>

</div>